<log4js:response state="Error"
	xmlns:log4js="http://stritti.github.io/log4js//2007/log4js/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://stritti.github.io/log4js//2007/log4js/ log4js.xsd">
	<log4js:message>All right!</log4js:message>
	<log4js:throwable>
		<log4js:message>An exception occurred.</log4js:message>
		<log4js:description>The long truth...</log4js:description>
		<log4js:stacktrace>
			<log4js:location class="this.is.my.class"
				fileName="class.java" lineNumber="43" method="aMethod" />
			<log4js:location class="this.is.another.classx"
				fileName="classx.java" lineNumber="57" method="callAMethod" />
		</log4js:stacktrace>
	</log4js:throwable>
</log4js:response>
